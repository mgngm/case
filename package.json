{
  "name": "tesseract",
  "version": "3.1.1",
  "private": true,
  "scripts": {
    "pre-commit": "lint-staged",
    "dev": "next dev",
    "dev:mock": "NEXT_PUBLIC_AMPLIFY_MOCK=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type": "tsc",
    "test": "jest",
    "sonarqube": "sonar-scanner -Dsonar.projectKey=WebUI:tesseract -Dsonar.sources=. -Dsonar.host.url=https://sonar.shared.actual.zone",
    "test:watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "merge": "./bin/update-env.sh",
    "prettier": "prettier --config .prettierrc.json --write",
    "regenerate-parse-lambda": "esbuild lambda/parse/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=amplify/backend/function/parseLambda/src/index.js",
    "regenerate-du-lambda": "esbuild lambda/du/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=amplify/backend/function/duLambda/src/index.js",
    "regenerate-publish-lambda": "esbuild lambda/publish/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=amplify/backend/function/publishLambda/src/index.js",
    "regenerate-dynamo-delete-lambda": "esbuild lambda/dynamoDelete/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=amplify/backend/function/dynamoDelete/src/index.js",
    "regenerate-filter-parse-lambda": "esbuild lambda/filterParse/index.ts --bundle  --sourcemap --platform=node --target=es2020 --outfile=amplify/backend/function/filterParseLambda/src/index.js --external:aws-serverless-express --external:aws-serverless-express/* --external:body-parser --external:express",
    "build:lambda": "npm run regenerate-parse-lambda; npm run regenerate-du-lambda; npm run regenerate-publish-lambda; npm run regenerate-dynamo-delete-lambda; npm run regenerate-filter-parse-lambda",
    "precodemod": "node transforms/build.js",
    "codemod": "jscodeshift",
    "pregenerate-graphql-map": "npm run precodemod",
    "generate-graphql-map": "node bin/generate-graphql-map.js"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^2.17.1",
    "@aws-sdk/client-cognito-identity-provider": "^3.118.1",
    "@aws-sdk/client-dynamodb": "^3.113.0",
    "@aws-sdk/client-s3": "^3.100.0",
    "@aws-sdk/client-sqs": "^3.231.0",
    "@aws-sdk/lib-dynamodb": "^3.113.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.10",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.92",
    "@mui/material": "^5.8.0",
    "@react-hook/resize-observer": "^1.2.6",
    "@reduxjs/toolkit": "^1.9.2",
    "@svgr/webpack": "^6.2.1",
    "@types/d3": "^7.4.0",
    "@types/json2csv": "^5.0.3",
    "apexcharts": "^3.35.2",
    "aws-amplify": "^4.3.22",
    "base-64": "^1.0.0",
    "clsx": "^1.1.1",
    "cookie": "^0.5.0",
    "crossfilter2": "^1.5.4",
    "csvtojson": "^2.0.10",
    "d3": "^7.4.4",
    "date-fns": "^2.29.3",
    "downloadjs": "^1.4.7",
    "formidable": "^3.2.4",
    "immer": "^9.0.17",
    "jest-environment-jsdom": "^28.1.0",
    "json2csv": "^5.0.7",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "next": "^12.3.4",
    "prismjs": "^1.28.0",
    "proxy-memoize": "^2.0.1",
    "qrcode.react": "^3.1.0",
    "react": "18.1.0",
    "react-apexcharts": "^1.4.0",
    "react-countup": "^6.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.31.2",
    "react-markdown": "^8.0.3",
    "react-persist": "^1.0.2",
    "react-redux": "^8.0.1",
    "react-simple-code-editor": "^0.11.2",
    "react-spinners": "^0.12.0",
    "react-xarrows": "^2.0.2",
    "sass": "^1.52.3",
    "universal-cookie": "^4.0.4",
    "use-immer": "^0.8.1",
    "uuid": "^8.3.2",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
